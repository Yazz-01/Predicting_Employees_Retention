    "df['CompanyIncomeyear'] = df['CompanyIncomeyear'].replace(np.inf,0)\n",
    "df['CompanyIncomeyear'] = df['CompanyIncomeyear'].replace(0,np.max(df['CompanyIncomeyear']))\n",
